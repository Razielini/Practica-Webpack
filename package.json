{
  "name": "practica-webpack",
  "version": "1.0.0",
  "description": "Una practica de Webpack",
  "main": "index.js",
  "scripts": {
    "build:manual": "webpack --entry ./src/index.js --output ./dist/bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:multiple": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loaders/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js",
    "build:prepro": "webpack-dev-server --config ./prepro/webpack.config.js",
    "build:prevent-duplication": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:dll": "webpack --config ./dinamic-link-library/webpack.dll.config.js",
    "build:dll-fast": "webpack --config ./dinamic-link-library/webpack.config.js",
    "build:dynamic:dll": "webpack --config ./dinamyc-import/webpack.dll.config.js",
    "build:dynamic:fast": "webpack --config ./dinamyc-import/webpack.config.js"
  },
  "author": "Razielini",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
